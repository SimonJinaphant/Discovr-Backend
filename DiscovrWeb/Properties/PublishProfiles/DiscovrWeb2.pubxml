    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DiscovrWebContext" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:discovrwebserver2.database.windows.net,1433;Initial Catalog=DiscovrWebDatabase2;Persist Security Info=True;User ID=discovr-admin;Password=M@pbox321;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="" />
