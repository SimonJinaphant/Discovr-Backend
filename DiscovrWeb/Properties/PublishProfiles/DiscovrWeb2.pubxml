  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DiscovrWebContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:discovrwebserver2.database.windows.net,1433;Initial Catalog=DiscovrWebDatabase2;Persist Security Info=True;User ID=discovr-admin;Password=M@pbox321;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False</ParameterValue>
    </MSDeployParameterValue>
