            <Source Path="DBMigration" DbContext="DiscovrWeb.Models.DiscovrWebContext, DiscovrWeb" MigrationConfiguration="DiscovrWeb.Migrations.Configuration, DiscovrWeb" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
